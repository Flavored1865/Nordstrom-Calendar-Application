<p id="notice"><%= notice %></p>

<h1>Schedules</h1>

<table class="blueTable">
  <thead>
  <tr>
    <th>Title</th>
    <th>Start</th>
    <th>End</th>
    <th>Color</th>
    <th>Created_At</th>
    <th>Updated_At</th>
    <th>Employees</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.title %></td>
      <td><%= event.start %></td>
      <td><%= event.end %></td>
      <td><%= event.color %></td>
      <td><%= event.created_at %></td>
      <td><%= event.updated_at %></td>
      <td><%= event.employee_id %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', employees_path %>